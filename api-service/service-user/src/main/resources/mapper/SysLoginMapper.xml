<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sinosdx.service.user.dao.mapper.SysLoginMapper">

    <select id="queryByMobile" resultType="com.sinosdx.service.user.dao.entity.SysLogin">
        select * from sys_login
        <where>
            mobile = #{mobile} and del_flag = 0
        </where>
    </select>
</mapper>